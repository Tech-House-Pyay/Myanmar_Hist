											<img src="<%= see[1].imgUrl %>"  height="200" width="250" alt="">
							         	</div>
						</div>
					</div>
					</div>
				<!-- single-feature -->
				<div class="col-sm-4">
					<div class="single-feature bg-light">
						<div class="feature-details">
							 <h6 class="text-danger"><%= see[2].categories %></h6><br><br>
							 <h6 class="text-danger"><%= see[2].title %></h6><br><br>
										<h6 class="text-danger"><%= see[2].content %></h6><br><br>
										<a href="#" class="primary-btn mb-40"><h6 class="text-danger">Read More</h6></a>
          <div class="feature-thumb">
								<img src="<%= see[2].imgUrl %>"  height="200" width="250" alt="">
						</div>
				</div>
			</div>
		</div>

</div>
	</div>
