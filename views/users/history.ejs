
<!doctype html>
<html lang="en">

<% include ../partialuser/head %>

<body>


			<% include ../partialuser/navbar %>



			<section class="home_banner_area section_gap_top" >
				<div class="banner_inner overlay d-flex align-items-center">
				<div class="container">
					<div class="row fullscreen d-flex align-items-center justify-content-center">
						<div class="banner_content">

							<h2>Myanmar History</h2>
						</div>
					</div>
				</div>
			</div>
			</section>
			<section class="feature-area section_gap_top">

							<div class="main_title">
               <h1 class="text-danger">The History of Myanmar</h1>
								<span class="title-widget-bg"></span>

					<h4 class="text-danger"><%= see[0].categories %></h4><br><br>
						 <h4 class="text-danger"><%= see[0].title %></h4><br><br>
						<h4 class="text-danger"><%= see[0].content %></h4><br><br>
									<center>	<a href="#" class="primary-btn mb-40" ><h4 class="text-danger">Read More</h4></a><br><br></center>
										<div class="feature-thumb">
									 <img src="%= see[1].imgUrl %>" alt="" width="250" height="200">
						</div>




			</div>

	</section>

<% include ../partialuser/script %>
</body>

</html>


