<% include ../partialadmin/adminhead %>

<body>
    <!-- Left Panel -->

<% include ../partialadmin/adminsidebar %>

    <!-- Left Panel -->

    <!-- Right Panel -->
<!-- /header -->
        <!-- Header-->

  <% include ../partialadmin/admintopbar %>

        <div class="content mt-3">
            <div class="animated fadeIn">
                <div class="row">

                    <div class="col-md-12">
                        <div class="card">
                            <div class="card-header">
                                <strong class="card-title">See and Do List</strong>
                            </div>
                            <div class="card-body bg-danger text-white">
                                <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <th>Categories</th>
                                            <th>Title</th>
                                            <th>Content</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                      <% for(var i=0; i<seeAdo.length; i++){%>
                                      <tr>
                                        <td class="text-white"> <a href="/seeAdo/seeAdodetail/<%=seeAdo[i]._id%>"><%= seeAdo[i].categories %></a></td>
                                        <td class="text-white"><%= seeAdo[i].title %></td>
                                        <td class="text-white"><%= seeAdo[i].content %></td>
                                      </tr>
                                        <%}%>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>


                </div>
            </div><!-- .animated -->
        </div><!-- .content -->


    </div><!-- /#right-panel -->

    <!-- Right Panel -->


  <% include ../partialadmin/adminscript %>

</body>

</html>


